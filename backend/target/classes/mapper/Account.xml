<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Account">

	<insert id="insertAccount" parameterType="Account">
		insert into
		accounts(account_email,account_password ,account_name
		,account_gitlab_email,account_role_name)
		values(#{account_email},#{account_password},#{account_name},#{account_gitlab_email},#{account_role_name});
	</insert>

	<select id="selectAccountByAccountEmail" resultType="Account" parameterType="String">
		select * from accounts where account_email = #{account_email}
	</select>

</mapper>

